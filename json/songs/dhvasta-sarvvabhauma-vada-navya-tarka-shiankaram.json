  "verses": [
    {
      "number": "21",
      "text": [
        "дгваста-са̄рвабгаума-ва̄да-навʼя-тарка-шянкарам",
        "дгваста-тад-віварта-ва̄да-да̄навīя-дамбарам",
        "даршіта̄ртха-сарва-шястра-кришна-бгакті-мандірам",
